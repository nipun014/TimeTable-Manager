# Environment variables for Timetable Solver

# Data file path (relative to timetable_solver directory)
DATA_FILE=sample_data.json

# Solver configuration
MAX_TIME_SECONDS=60
NUM_WORKERS=8
LOG_PROGRESS=true
RANDOM_SEED=42

# Output files
TIMETABLE_IMAGE=timetable.png
TEACHER_TIMETABLE_IMAGE=teacher_timetables.png
ROOM_TIMETABLE_IMAGE=room_timetables.png
SOLUTION_JSON=solution.json

# Streamlit configuration
STREAMLIT_PORT=8501
STREAMLIT_HEADLESS=true